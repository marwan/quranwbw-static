{"code":200,"data":{"verses":{"100:1":{"meta":{"chapter":100,"verse":1,"page":599,"juz":30,"hizb":239,"manzil":7,"ruku":542,"words":2},"words":{"arabic":"وَٱلۡعَٰدِيَٰتِ|ضَبۡحٗا","translation":"By the racers|panting","transliteration":"wal-'adiyati|dabhan","timestamp":"0.1|1.8","line":"14|14","end":"١","end_line":14},"translations":{"1":"Wal'aadi-yaati dabha","15":"’ By the galloping, panting horses"}},"100:2":{"meta":{"chapter":100,"verse":2,"page":599,"juz":30,"hizb":239,"manzil":7,"ruku":542,"words":2},"words":{"arabic":"فَٱلۡمُورِيَٰتِ|قَدۡحٗا","translation":"And the producers of sparks|striking","transliteration":"fal-muriyati|qadhan","timestamp":"0.04|1.7","line":"14|14","end":"٢","end_line":14},"translations":{"1":"Fal-moori-yaati qadha","15":"striking sparks of fire ˹with their hoofs˺"}},"100:3":{"meta":{"chapter":100,"verse":3,"page":599,"juz":30,"hizb":239,"manzil":7,"ruku":542,"words":2},"words":{"arabic":"فَٱلۡمُغِيرَٰتِ|صُبۡحٗا","translation":"And the chargers|(at) dawn","transliteration":"fal-mughirati|sub'han","timestamp":"0.03|1.6","line":"14|15","end":"٣","end_line":15},"translations":{"1":"Fal-mugheeraati subha","15":"launching raids at dawn"}},"100:4":{"meta":{"chapter":100,"verse":4,"page":599,"juz":30,"hizb":239,"manzil":7,"ruku":542,"words":3},"words":{"arabic":"فَأَثَرۡنَ|بِهِۦ|نَقۡعٗا","translation":"Then raise|thereby|dust","transliteration":"fa-atharna|bihi|naq'an","timestamp":"0.03|1|1.7","line":"15|15|15","end":"٤","end_line":15},"translations":{"1":"Fa-atharna bihee naq'a","15":"stirring up ˹clouds of˺ dust"}},"100:5":{"meta":{"chapter":100,"verse":5,"page":599,"juz":30,"hizb":239,"manzil":7,"ruku":542,"words":3},"words":{"arabic":"فَوَسَطۡنَ|بِهِۦ|جَمۡعًا","translation":"Then penetrate (in the) center|thereby|(of) troops","transliteration":"fawasatna|bihi|jam'an","timestamp":"0.04|1|1.7","line":"15|15|15","end":"٥","end_line":15},"translations":{"1":"Fawa-satna bihee jam'a","15":"and penetrating into the heart of enemy lines"}},"100:6":